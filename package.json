{
  "name": "ui-template",
  "private": true,
  "version": "1.0.31",
  "type": "module",
  "scripts": {
    "dev": "sst dev vite",
    "dev:local": "vite",
    "server": "node server.mjs",
    "start": "concurrently \"vite\" \"node server.mjs\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "echo \"No tests configured\" && exit 0",
    "preview": "vite preview",
    "serve": "npm run build && node server.mjs",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "setup": "./scripts/setup.sh",
    "deploy": "npm run version:patch && sst deploy",
    "deploy:dev": "npm run version:patch && sst deploy --stage dev",
    "deploy:int": "npm run version:patch && sst deploy --stage int",
    "deploy:prod": "npm run version:patch && sst deploy --stage prod",
    "remove": "sst remove",
    "remove:dev": "sst remove --stage dev",
    "remove:int": "sst remove --stage int",
    "remove:prod": "sst remove --stage prod"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.0.60",
    "@headlessui/react": "^2.2.9",
    "@reduxjs/toolkit": "^2.11.0",
    "aws-amplify": "^6.12.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.6",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "sst": "^3.17.24",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
